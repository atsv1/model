            <ForEach enable = "enableFlag" arrayvalue = "tempValue" coordinate = "i" coordinate1 = "j">
            <![CDATA[
              tempValue := i + j;

           ]]>
          </ForEach>

        </Param>
        <Param name = "funcVar" type = "integer" initvalue = "7">
        </Param>
        <Param name = "var" type = "integer" initvalue = "0">
          <Formula>
            <![CDATA[
                 {i := getSumm(1, 1);}

                 var := getSumm(mas[1,1], mas[2,2]) ;

           ]]>
